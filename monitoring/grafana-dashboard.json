{
  "dashboard": {
    "title": "Event Processing Platform",
    "panels": [
      {
        "title": "Request Rate",
        "targets": [
          {
            "expr": "rate(api_requests_total[5m])"
          }
        ]
      },
      {
        "title": "P99 Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(api_request_latency_seconds_bucket[5m]))"
          }
        ]
      },
      {
        "title": "Queue Depth",
        "targets": [
          {
            "expr": "aws_sqs_approximate_number_of_messages_visible"
          }
        ]
      },
      {
        "title": "Worker Count",
        "targets": [
          {
            "expr": "worker_active_count"
          }
        ]
      },
      {
        "title": "Events Processed",
        "targets": [
          {
            "expr": "rate(worker_events_processed_total[5m])"
          }
        ]
      },
      {
        "title": "Error Rate",
        "targets": [
          {
            "expr": "rate(event_errors_total[5m])"
          }
        ]
      }
    ]
  }
}
